/* Write a program to print the sum of two numbers by taking values from user. */

import java.util.*;
public class Program2 {
    public static void main(String[] args)
    {
        Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        int b = scan.nextInt();

        System.out.println("Sum is : "+(a+b));
    }
}
